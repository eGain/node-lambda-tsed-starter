    "@tsed/json-mapper": "7.84.0",
    "@tsed/logger": "6.7.5",
    "@tsed/openspec": "7.84.0",
    "@tsed/platform-exceptions": "7.84.0",
    "@tsed/platform-params": "7.84.0",
    "@tsed/platform-response-filter": "7.84.0",
    "@tsed/schema": "7.84.0",
    "@tsed/swagger": "7.84.0",
    "ajv": "^8.17.1",
    "body-parser": "^1.20.3",
    "compression": "^1.7.4",
