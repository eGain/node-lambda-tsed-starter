{
  "name": "@project/commands",
  "version": "1.0.0",
  "description": "Package to bind commands line to a Ts.ED project",
  "type": "module",
  "scripts": {
    "test": "vitest run",
    "test:watch": "vitest",
    "build": "tsc --project tsconfig.node.json",
    "run-cmd": "CI=1 node --import @swc-node/register/esm-register bin/index.ts",
    "timeslots": "yarn run-cmd timeslots --limit 5",
    "timeslots:load": "yarn run-cmd timeslots-load",
    "generate:token:timeslots": "yarn run-cmd generate-token test@egain.com -s timeslots"
  },
  "dependencies": {
    "@faker-js/faker": "8.4.1",
    "@project/controllers": "workspace:*",
    "@tsed/ajv": "8.0.0-rc.2",
    "@tsed/cli-core": "6.0.0-rc.1",
    "@tsed/core": "8.0.0-rc.2",
    "@tsed/di": "8.0.0-rc.2",
    "@tsed/engines": "8.0.0-rc.2",
    "@tsed/exceptions": "8.0.0-rc.2",
    "@tsed/json-mapper": "8.0.0-rc.2",
    "@tsed/logger": "6.7.5",
    "@tsed/openspec": "8.0.0-rc.2",
    "@tsed/platform-exceptions": "8.0.0-rc.2",
    "@tsed/platform-params": "8.0.0-rc.2",
    "@tsed/platform-response-filter": "8.0.0-rc.2",
    "@tsed/schema": "8.0.0-rc.2",
    "@tsed/swagger": "8.0.0-rc.2",
    "ajv": "^8.17.1",
    "body-parser": "^1.20.3",
    "compression": "^1.7.4",
    "cookie-parser": "^1.4.7",
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "dotenv-expand": "^11.0.6",
    "dotenv-flow": "^4.1.0",
    "express": "^4.21.1",
    "method-override": "^3.0.0"
  },
  "devDependencies": {
    "@swc-node/register": "^1.10.9",
    "@swc/cli": "^0.5.0",
    "@swc/core": "^1.7.42",
    "@swc/helpers": "^0.5.13",
    "@tsed/cli": "^6.0.0-rc.1",
    "@tsed/cli-generate-swagger": "^6.0.0-rc.1",
    "@tsed/cli-plugin-eslint": "6.0.0-rc.1",
    "@tsed/cli-plugin-vitest": "6.0.0-rc.1",
    "@tsed/cli-testing": "6.0.0-rc.1",
    "@tsed/platform-express": "^8.0.0-rc.2",
    "@tsed/platform-views": "^8.0.0-rc.2",
    "@types/compression": "^1.7.5",
    "@types/cookie-parser": "^1.4.7",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/method-override": "^0.0.35",
    "@types/multer": "^1.4.12",
    "nodemon": "3.1.7",
    "supertest": "^6.3.4",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "typescript": "5.6.3",
    "vitest": "2.1.4"
  }
}
